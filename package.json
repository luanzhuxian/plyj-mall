{
  "name": "plyj-mall",
  "version": "2.1.4",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve --model local",
    "build": "vue-cli-service build --model production",
    "build:dev": "vue-cli-service build --model development",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "svg": "vsvg -s ./src/assets/svg -t ./src/assets/fonts/svg",
